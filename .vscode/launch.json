{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "java",
      "name": "Run JUMO (uses SUMO_HOME)",
      "request": "launch",
      "mainClass": "rt.traffic.AppMain",
      "cwd": "${workspaceFolder}",
      "vmArgs": "--enable-native-access=ALL-UNNAMED -Djava.library.path=${env:SUMO_HOME}/lib:${env:SUMO_HOME}/bin"
    }
  ]
}





